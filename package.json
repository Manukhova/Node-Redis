{
  "name": "ajson-forms",
  "version": "0.0.2",
  "description": "Proxy service for processing website forms data",
  "private": true,
  "main": "index.js",
  "devDependencies": {
    "colors": "^1.3.3",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-prettier": "^2.5.0",
    "jest": "^21.2.1",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1"
  },
  "dependencies": {
    "express": "^4.16.2",
    "redis": "^2.8.0",
    "uuid": "^3.2.1",
    "winston": "^3.0.0"
  },
  "scripts": {
    "start": "node ./index.js",
    "errors": "node ./index.js --getErrors",
    "test": "jest",
    "lint": "eslint './{src,configs}/**/*.js'",
    "coverage": "jest --coverage",
    "format": "prettier --write --single-quote --print-width=120 --no-editorconfig './{src,configs}/**/*.js'"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|coverage)[/\\\\]"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "./jestsetup.js"
    ],
    "collectCoverageFrom": [
      "./**/*.js",
      "!**/coverage/**"
    ]
  }
}
